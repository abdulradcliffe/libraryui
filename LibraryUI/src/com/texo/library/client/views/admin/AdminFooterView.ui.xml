<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:m='urn:import:gwt.material.design.client.ui'>
	<ui:style>
	.welcomeLabel {
		font-size: small;
		color: white;
		position: absolute;
		left: 25px;
		top: 10px;
	}
	
	.logoutButton {
		position: absolute;
		right: 20px;
		top: 10px;
	}
	
	.root {
		width: 1200px;
		position: absolute;
		background-color: #464646;
		padding: 0px;
		height: 40px;
		top: 570px;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{style.root}">
			<g:Label styleName="{style.welcomeLabel}">Copyright</g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>