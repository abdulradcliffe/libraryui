<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:m='urn:import:gwt.material.design.client.ui'>
	<ui:style>
	.middlePanel {
		height: 500px;
		position: absolute;
		top: 70px;
		width: 1200px;
	}
	
	.adminNavigationPanel {
		position: absolute;
		width: 200px;
		height: 500px;
	}
	
	.adminMainView {
		position: absolute;
		width: 1000px;
		left: 200px;
		height: 500px;
		overflow-y: scroll;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{style.middlePanel}">
			<g:HTMLPanel ui:field="adminNavigationPanel"
				styleName="{style.adminNavigationPanel}"></g:HTMLPanel>
			<g:HTMLPanel ui:field="adminMainView"
				styleName="{style.adminMainView}"></g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>